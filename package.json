{
  "name": "mongo-utils",
  "version": "0.0.1",
  "description": "Handy CLI utils for managing multiple mongo instances",
  "keywords": [
    "mongoose",
    "utils"
  ],
  "homepage": "https://github.com/yashshah1/mongo-utils#readme",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "start": "nodemon index.js",
    "build": "npm run compile",
    "compile": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "precommit": "npm run format && npm run lint && npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format && npm run lint"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Yash Shah <yashah1234@gmail.com>",
  "contributors": [
    {
      "name": "Savan Nahar",
      "email": "savannahar68@gmail.com"
    },
    {
      "name": "Pranav Joglekar"
    },
    {
      "name": "Vasu Sharma",
      "email": "vasusharma2017@outlook.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yashshah1/mongo-utils.git"
  },
  "bugs": {
    "url": "https://github.com/yashshah1/mongo-utils/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "typescript": "^3.8.3",
    "uglify-js": "^3.9.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run format && npm run lint && npm run build"
    }
  },
  "dependencies": {
    "blob": "^0.1.0",
    "json-2-csv": "^3.7.7",
    "json-as-xlsx": "^1.1.5",
    "mongodb": "^3.6.2"
  }
}
